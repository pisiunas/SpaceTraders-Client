insert_final_newline=true

# http://kent-boogaart.com/blog/editorconfig-reference-for-c-developers#tab_width
tab_width=4

# Modifiers' order
csharp_preferred_modifier_order=public, private, protected, internal, new, abstract, virtual, sealed, override, static, readonly, extern, unsafe, volatile, async:warning

# http://kent-boogaart.com/blog/editorconfig-reference-for-c-developers#dotnet_naming_
# Naming

# Private fields must be _camelCase
# Define private fields symbols
dotnet_naming_symbols.private_fields.applicable_kinds=field
dotnet_naming_symbols.private_fields.applicable_accessibilities=private
# Define private fields style
dotnet_naming_style.underscore_camelcase.capitalization=camel_case
dotnet_naming_style.underscore_camelcase.required_prefix=_
# Define private fields naming rules
dotnet_naming_rule.private_fields_must_be_camelcase_prefixed_with_underscore.symbols=private_fields
dotnet_naming_rule.private_fields_must_be_camelcase_prefixed_with_underscore.style=underscore_camelcase
dotnet_naming_rule.private_fields_must_be_camelcase_prefixed_with_underscore.severity=warning

# Non private fields must be PascalCase
# Define non private fields symbols
dotnet_naming_symbols.non_private_fields.applicable_kinds=field
dotnet_naming_symbols.non_private_fields.applicable_accessibilities=public,internal,protected,protected_internal
# Define non private fields style
dotnet_naming_style.pascal_case_style.capitalization=pascal_case
# Define non private fields naming rules
dotnet_naming_rule.non_private_fields_must_be_pascal_case.symbols=non_private_fields
dotnet_naming_rule.non_private_fields_must_be_pascal_case.style=pascal_case_style
dotnet_naming_rule.non_private_fields_must_be_pascal_case.severity=warning

# Parameters must be camelCase
# Define parameters' symbols
dotnet_naming_symbols.parameters.applicable_kinds=parameter
dotnet_naming_symbols.parameters.applicable_accessibilities=*
# Define parameters' style
dotnet_naming_style.camel_case_style.capitalization=camel_case
# Define parameters' rules
dotnet_naming_rule.parameters_must_be_camelcase.symbols=parameter
dotnet_naming_rule.parameters_must_be_camelcase.style=camel_case_style
dotnet_naming_rule.parameters_must_be_camelcase.severity=warning

# Non-interface types and namespace must be PascalCase
# Define non-interface type symbols
dotnet_naming_symbols.non_interface_types.applicable_kinds=namespace,class,struct,enum,delegate
dotnet_naming_symbols.non_interface_types.applicable_accessibilities=*
# Define non-interface types' rules
dotnet_naming_rule.non_interface_types_must_be_pascal_case.symbols=non_interface_types
dotnet_naming_rule.non_interface_types_must_be_pascal_case.style=pascal_case_style
dotnet_naming_rule.non_interface_types_must_be_pascal_case.severity=warning

# Interface types must be IPascalCase
# Define interface type symbols
dotnet_naming_symbols.interface_types.applicable_kinds=interface
dotnet_naming_symbols.interface_types.applicable_accessibilities=*
# Define interfaces style
dotnet_naming_style.i_pascal_case.capitalization=pascal_case
dotnet_naming_style.i_pascal_case.required_prefix=I
# Define interface rules
dotnet_naming_rule.interface_types_must_be_pascal_case_prefixed_with_i.symbols=interface_types
dotnet_naming_rule.interface_types_must_be_pascal_case_prefixed_with_i.style=i_pascal_case
dotnet_naming_rule.interface_types_must_be_pascal_case_prefixed_with_i.severity=warning

# Members must be PascalCase
# Define member type symbols
dotnet_naming_symbols.members.applicable_kinds=property,event,method,local_function,type_parameter
dotnet_naming_symbols.members.applicable_accessibilities=*
# Define members' rules
dotnet_naming_rule.members_must_be_pascal_case.symbols=members
dotnet_naming_rule.members_must_be_pascal_case.style=pascal_case_style
dotnet_naming_rule.members_must_be_pascal_case.severity=warning

# Constants should be PascalCase
# Define constants' symbols
dotnet_naming_symbols.constants.applicable_kinds=field
dotnet_naming_symbols.constants.applicable_accessibilities=*
dotnet_naming_symbols.constants.required_modifiers=const
# Define constants' rules
dotnet_naming_rule.constants_must_be_pascal_case.symbols=constants
dotnet_naming_rule.constants_must_be_pascal_case.style=pascal_case_style
dotnet_naming_rule.constants_must_be_pascal_case.severity=warning

# General rules
csharp_preferred_modifier_order=public, private, protected, internal, new, abstract, virtual, sealed, override, static, readonly, extern, unsafe, volatile, async:warning

dotnet_style_require_accessibility_modifiers=for_non_interface_members:warning

# roslynator

roslynator_compiler_diagnostic_fixes.enabled = true

roslynator_binary_operator_new_line = after
dotnet_diagnostic.rcs0027.severity = warning

roslynator_arrow_token_new_line = before
dotnet_diagnostic.rcs0032.severity = warning

roslynator_new_line_at_end_of_file = true
dotnet_diagnostic.rcs0058.severity = warning

# Add blank line between closing brace and next statement
dotnet_diagnostic.rcs0008.severity = warning

# Add blank line between declarations
dotnet_diagnostic.rcs0010.severity = warning

# Use spaces instead of tab
dotnet_diagnostic.rcs0046.severity = warning

# Fix formatting of a call chain
dotnet_diagnostic.rcs0054.severity = warning

# Other file types
[*.{appxmanifest,axml,build,config,csproj,dbml,discomap,dtd,jsproj,lsproj,njsproj,nuspec,proj,props,proto,resw,StyleCop,targets,tasks,vbproj,xml,xsd}]
indent_style=space
indent_size=4
tab_width=4

[*.{asax,ascx,aspx,cs,cshtml,css,htm,html,js,jsx,master,razor,skin,ts,tsx,vb,xaml,xamlx,xoml}]
indent_style=space
indent_size=4
tab_width=4

[*.{json,resjson,resx}]
indent_style=space
indent_size=2
tab_width=2